{
  "name": "saas-web-client",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "compile:server": "webpack --config webpack.server.config.js --progress --colors",
    "serve:ssr": "node dist/server",
    "build:ssr": "npm run build:client-and-server-bundles && npm run compile:server",
    "build:client-and-server-bundles": "ng build --aot --prod && ng run saasWebClient:server:production",
    "build:ssr-bg": "npm run build:client-and-server-bundles-bg && npm run compile:server",
    "build:client-and-server-bundles-bg": "ng build --configuration=production-bg && ng run saasWebClient:server:production-bg",
    "extract-i18n": "ng xi18n saasWebClient --i18n-format xlf --output-path locale --i18n-locale en && ng run saasWebClient:xliffmerge"
  },
  "private": true,
  "dependencies": {
    "@agm/core": "^1.0.0",
    "@angular/animations": "~8.2.9",
    "@angular/cdk": "^8.2.3",
    "@angular/common": "~8.2.9",
    "@angular/compiler": "~8.2.9",
    "@angular/core": "~11.0.5",
    "@angular/forms": "~8.2.9",
    "@angular/http": "~7.2.14",
    "@angular/platform-browser": "~8.2.9",
    "@angular/platform-browser-dynamic": "~8.2.9",
    "@angular/platform-server": "~8.2.9",
    "@angular/router": "~8.2.9",
    "@hapiness/ng-universal-transfer-http": "^10.1.1",
    "@nguniversal/common": "^9.0.2",
    "@nguniversal/express-engine": "^8.1.1",
    "@nguniversal/module-map-ngfactory-loader": "8.1.1",
    "@ngx-i18nsupport/tooling": "^8.0.3",
    "@ngx-translate/i18n-polyfill": "^1.0.0",
    "@types/gapi": "0.0.39",
    "@types/gapi.auth2": "0.0.50",
    "@types/googlemaps": "^3.37.7",
    "@types/stripe": "^6.32.8",
    "compression": "^1.7.4",
    "core-js": "^3.2.1",
    "express": "^4.17.1",
    "hammerjs": "^2.0.8",
    "linkifyjs": "^2.1.8",
    "ng-zorro-antd": "^8.4.1",
    "ngx-cacheable": "^1.3.0",
    "ngx-quill": "^8.0.0",
    "preboot": "^7.0.0",
    "quill": "^1.3.7",
    "rxjs": "~6.5.3",
    "source-map-explorer": "^2.1.0",
    "tslib": "^1.10.0",
    "zone.js": "^0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.25",
    "@angular/cli": "~8.3.8",
    "@angular/compiler-cli": "~8.2.9",
    "@angular/language-service": "~8.2.9",
    "@ngx-i18nsupport/ngx-i18nsupport": "^1.1.6",
    "@types/jasmine": "~3.4.2",
    "@types/jasminewd2": "~2.0.8",
    "@types/node": "~12.7.11",
    "codelyzer": "~5.1.2",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "protractor": "~5.4.0",
    "sass": "^1.26.8",
    "tar": "5.0.5",
    "ts-loader": "^6.2.2",
    "ts-node": "~8.4.1",
    "tslint": "~5.20.0",
    "typescript": "~3.5.3",
    "webpack-cli": "^3.3.9"
  },
  "resolutions": {
    "@babel/preset-env": "^7.8.7"
  }
